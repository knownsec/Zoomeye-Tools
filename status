[32m+[m[32m            $("#hostlink")[0].href=`https://www.zoomeye.org/searchResult?q=site: ${TARGET_HOST}`;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        $("#loaded").show();[m[41m[m
[32m+[m[32m        $("#view").show();[m[41m[m
[32m+[m[32m        $("#loading").hide();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function isValIP(ip){[m[41m[m
[32m+[m[32m        if((ip.match(/^(127|192)\./))){[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        else if(ip.match(/^(172\.(1[6-9]|2[0-9]|3[0-2]))/)){[m[41m[m
[32m+[m[32m            return false[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        else if(ip.match(/^10\./)){[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        else{[m[41m[m
[32m+[m[32m            return true;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    function isValUrl(url){[m[41m[m
[32m+[m[32m        if((url.match(/^chrome:\/\/extensions/))||(url.match(/^chrome:\/\//))){[m[41m[m
[32m+[m[32m            return false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        if((url.match(/^file:/))){[m[41m[m
[32m+[m[32m            return false[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        else{[m[41m[m
[32m+[m[32m            return true;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    chrome.tabs.query({active: true, lastFocusedWindow: true}, tabs => {[m[41m[m
[32m+[m[32m        let url = tabs[0].url;[m[41m[m
[32m+[m[32m        if(!isValUrl(url)){[m[41m[m
[32m+[m[32m            $("#down").show();[m[41m[m
[32m+[m[32m            $("#loading").hide();[m[41m[m
[32m+[m[32m            $("#loaded").hide();[m[41m[m
[32m+[m[32m            $("#view").hide();[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        TARGET_HOST = url.match(/^(?:http|https):\/\/(.*?)\//)[1];[m[41m[m
[32m+[m[32m        if(!isValIP(TARGET_HOST)){[m[41m[m
[32m+[m[32m            $("#down").show();[m[41m[m
[32m+[m[32m            $("#loading").hide();[m[41m[m
[32m+[m[32m            $("#loaded").hide();[m[41m[m
[32m+[m[32m            $("#view").hide();[m[41m[m
[32m+[m[32m            return;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        $("#loading").show();[m[41m[m
[32m+[m[32m        $("#loaded").hide();[m[41m[m
[32m+[m[32m        $("#view").hide();[m[41m[m
[32m+[m[32m        getStorageSite(TARGET_HOST);[m[41m[m
[32m+[m[32m    });[m[41m[m
 }[m
